public class Toolbar extends JPanel{
	
	private JButton hellowButton;
	private JButton goodByeButton;
	
	
	
	public Toolbar(){
		
		hellowButton = new JButton("hello");
		
		goodByeButton = new JButton("Good bye");		
	}

}
